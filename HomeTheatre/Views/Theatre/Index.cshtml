
@{
    ViewData["Title"] = "Index";
}

<head>
    <link rel="stylesheet" href="~/css/StyleParams.css" />
</head>

<div class="page-header header-filter" data-parallax="true">
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <h1 class="title">
                    Theatres
                </h1>
                <br />
                <h5>Header Five</h5>
            </div>
        </div>
    </div>
</div>
<div class="main main-raised">
    <div class="profile-content">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div>
                        <br />
                        @if (User.IsInRole("Administrator"))
                        {
                            <partial name="CreateTheatrePartialView" />
                        }
                        <br />
                        <div id="bartableplaceholder">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
